[app]
# (Mandatory) Name of the application
title = GeneratorAutomationApp
# (Mandatory) Package
package.name = generatorautomationapp
package.domain = org.myapp
# (Mandatory) Application version
version = 1.0.0

# (Mandatory) Python version to use
python.version = 3
# (Mandatory) List of source files
source.include_exts = py,png,jpg,kv,atlas

# (Mandatory) Android permissions
android.permissions = INTERNET,ACCESS_NETWORK_STATE

# (Optional) Android specific configuration
#android.sdk_path = /path/to/android/sdk
#android.ndk_path = /path/to/android/ndk
